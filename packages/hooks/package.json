{
	"name": "vue-hooks-plus",
	"version": "1.1.4",
	"files": [
    "dist",
    "lib",
    "es",
    "types",
    "metadata.json",
    "package.json",
    "README.md" 
	],
	"main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "unpkg": "dist/index.es.js",
	"keywords": [
		"vue3",
		"vue hooks",
		"typeScript"
	],
	"scripts": {
		"initial": "initial-scan .docs",
		"docs:dev": " vitepress dev .docs --host",
		"docs:build": "cross-env NODE_ENV=production vitepress build .docs",
		"docs:serve": "vitepress serve .docs --host",
    "clean": "rimraf dist lib es",
		"build": "npm run clean  && vue-tsc --noEmit && vite build",
		"build:types": "node scripts/build-types.js",
		"type": "tsc -d",
		"test": "npm run clean  && jest",
		"preview": "vite preview",
		"deply": "chmod +x ./deply.sh  && ./deply.sh"
	},
	"peerDependencies": {
		"vue": "^3.2.25"
	},
	"dependencies": {
		"js-cookie": "^2.x.x",
		"lodash": "^4.17.21",
		"marked": "^4.0.17",
		"query-string": "^7.1.1"
	},
	"devDependencies": {
		"@types/jest": "^27.4.1",
		"@types/js-cookie": "^3.0.1",
		"@types/lodash": "^4.14.178",
		"@types/marked": "^4.0.3",
		"@types/node": "^17.0.21",
		"@vitejs/plugin-vue": "^2.0.0",
		"@vue-hooks-plus/md-demo-plugins": "^1.0.0",
		"@vue-hooks-plus/vite-plugin-gen-temp": "1.4.0",
		"@vue-hooks-plus/vitepress-demo-block": "^1.0.0",
		"cross-env": "^7.0.3",
		"gh-pages": "^4.0.0",
		"jest": "^27.4.1",
		"less": "^4.1.3",
		"prettier": "^1.19.1",
		"rimraf": "^3.0.2",
		"ts-jest": "^27.1.4",
		"ts-morph": "^13.0.2",
		"ts-node": "^10.7.0",
		"typescript": "^4.4.4",
		"vite": "^3.0.2",
		"vite-plugin-build": "^0.6.0",
		"vite-plugin-dts": "^1.4.1",
		"vitepress": "1.0.0-alpha.4",
