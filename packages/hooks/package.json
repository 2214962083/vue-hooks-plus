{
	"name": "vue3-hooks-plus",
	"version": "2.2.8",
	"files": [
		"dist",
		"package.json"
	],
	"module": "dist/js/index.cjs.js",
	"main": "dist/js/index.es.js",
	"types": "dist/lib/src/index.d.ts",
	"keywords": [
		"vue3",
		"vue3 hooks",
		"typeScript"
	],
	"scripts": {
		"dev": "vitepress-fc dev docs --host",
		"build": "vue-tsc --noEmit && vite build && node scripts/build-types.js",
		"docs-build": "rimraf docs/dist && cross-env NODE_ENV=production vitepress-fc build --root=docs",
		"preview": "vite preview",
		"build:types": "node scripts/build-types.js",
		"deply": "chmod +x ./deply.sh  && ./deply.sh"
	},
	"peerDependencies": {
		"vue": "^3.2.25"
	},
	"dependencies": {
		"@types/js-cookie": "^3.0.1",
		"echarts": "^5.3.0",
		"js-cookie": "^2.x.x",
		"lodash": "^4.17.21",
		"query-string": "^7.1.1"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.178",
		"@types/node": "^17.0.21",
		"@vitejs/plugin-vue": "^2.0.0",
		"cross-env": "^7.0.3",
		"prettier": "^1.19.1",
		"rimraf": "^3.0.2",
		"ts-morph": "^13.0.2",
		"typescript": "^4.4.4",
		"vite": "^2.7.2",
		"vite-plugin-dts": "^0.9.9",
		"vitepress-for-component": "^0.16.6",
		"vue": "^3.2.25",
		"vue-tsc": "^0.29.8"
	}
}
