{
	"name": "vue3-hooks-plus",
	"version": "2.4.4",
	"files": [
		"dist",
		"package.json"
	],
	"module": "dist/js/index.cjs.js",
	"main": "dist/js/index.es.js",
	"types": "dist/src/index.d.ts",
	"keywords": [
		"vue3",
		"vue3 hooks",
		"typeScript"
	],
	"scripts": {
		"vite": "vitepress-fc dev .docs --host",
		"dev": "vitepress-fc dev docs --host",
		"site:dev": "initial-scan .docs && vitepress dev .docs --host",
		"site:build": "cross-env NODE_ENV=production vitepress build .docs",
		"build": "vue-tsc --noEmit && vite build",
		"type": "tsc -d",
		"test": "jest",
		"docs-serve": "cross-env NODE_ENV=production vitepress-fc serve --root=docs",
		"docs-build": "rimraf docs/dist && cross-env NODE_ENV=production vitepress-fc build --root=docs",
		"preview": "vite preview",
		"build:types": "node scripts/build-types.js",
		"deply": "chmod +x ./deply.sh  && ./deply.sh"
	},
	"peerDependencies": {
		"vue": "^3.2.25"
	},
	"dependencies": {
		"js-cookie": "^2.x.x",
		"lodash": "^4.17.21",
		"marked": "^4.0.17",
		"query-string": "^7.1.1"
	},
	"devDependencies": {
		"@types/jest": "^27.4.1",
		"@types/js-cookie": "^3.0.1",
		"@types/lodash": "^4.14.178",
		"@types/marked": "^4.0.3",
		"@types/node": "^17.0.21",
		"@vitejs/plugin-vue": "^2.0.0",
		"jest": "^27.4.1",
		"prettier": "^1.19.1",
		"rimraf": "^3.0.2",
		"ts-jest": "^27.1.4",
		"ts-morph": "^13.0.2",
		"ts-node": "^10.7.0",
		"typescript": "^4.4.4",
		"vite-plugin-dts": "^0.9.10",
		"vitepress-for-component": "^0.17.1",
		"vue": "^3.2.25",
		"vue-tsc": "^0.29.8",
		"@vfc/md-demo-plugins": "workspace:^1.0.0",
		"@vfc/vite-plugin-gen-temp": "workspace:^1.0.0",
		"@vfc/vitepress-demo-block": "workspace:^1.0.0",
		"cross-env": "^7.0.3",
		"gh-pages": "^4.0.0",
		"vite": "^3.0.2",
		"vitepress": "1.0.0-alpha.4",
		"vue-typical": "^2.1.0"
	},
	"repository": "https://github.com/InhiblabCore/vue3-hooks-plus",
	"homepage": "https://github.com/InhiblabCore/vue3-hooks-plus"
}
